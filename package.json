{
  "name": "battery-simulation-tool-frontend",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "run:dev": "vite --port 8000 --mode dev",
    "run:json": "nodemon json-server/server.mjs -w json-server & vite --port 8000 --mode dev-json",
    "build": "vite build",
    "lint": "eslint --ext js,mjs,cjs,vue --ignore-path .gitignore .",
    "test": "vitest run",
    "hooks": "husky install",
    "prettier-format": "prettier -w \"{json-server,src}/**/*.{js,vue,json,html,css,md}\"",
    "cy:open": "cypress open"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.6",
    "@repowerednl/ui-component-library": "^0.3.32",
    "@sentry/vite-plugin": "^2.16.1",
    "@sentry/vue": "^7.114.0",
    "@tailwindcss/forms": "^0.5.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "chart.js": "^4.4.2",
    "chartjs-adapter-luxon": "^1.3.1",
    "date-fns": "^3.6.0",
    "dompurify": "^3.1.3",
    "dotenv": "^16.4.5",
    "html-to-image": "^1.11.11",
    "html2pdf": "^0.0.11",
    "html2pdf.js": "^0.10.1",
    "json-server": "^0.17.4",
    "jspdf": "^2.5.1",
    "leaflet": "^1.9.4",
    "luxon": "^3.4.4",
    "marked": "^12.0.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "plotly.js-dist-min": "^2.32.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.8",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.1",
    "vue-json-csv": "^2.1.0",
    "vue-router": "4.3.0",
    "xlsx": "^0.18.5",
    "yarn.lock": "^0.0.1-security"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@testing-library/vue": "^8.0.3",
    "@vitest/coverage-v8": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "cypress": "^13.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.24.1",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "postcss-nesting": "^12.1.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "~5.4.4",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "{json-server,src}/**/*.{js,mjs,cjs,vue,json,html,css,md}": [
      "yarn prettier --write --list-different"
    ],
    "{json-server,src}/**/*.{js,mjs,cjs,vue}": [
      "yarn eslint --fix --ignore-path .gitignore"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "packageManager": "yarn@4.1.1"
}
